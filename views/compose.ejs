<%- include('partials/header'); -%>

<h1>Compose</h1>


<form class="item" action="/compose" method="POST">
    <div class="form-group">
        <h5>Title</h5>

        <input type="text" class="form-control" name="postTitle" autocomplete="off">



        <h5>Post</h5>
        <textarea class="form-control" name="postBody" rows="3" autocomplete="off"></textarea>


    </div>

    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>



<%- include('partials/footer'); -%>